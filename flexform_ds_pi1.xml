<T3DataStructure>
 <meta>
     <langDisable type="integer">1</langDisable>
 </meta>
 <sheets>
  <sDEF>
   <ROOT>
    <TCEforms>
     <sheetTitle>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
    </TCEforms>
    <type>array</type>
     <el>
        <less_url>
         <TCEforms>
          <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.1_sheet_url</label>
          <onChange>reload</onChange>
          <config>
            <type>input</type>
            <size>30</size>
          </config>
         </TCEforms>
        </less_url>

        <templateId>
          <TCEforms>
           <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.template_name</label>
           <onChange>reload</onChange>
           <config>
            <type>select</type>
             <itemsProcFunc>tx_templates_webservice->getTemplateNames</itemsProcFunc>
           </config>
           </TCEforms>
        </templateId>
         
        <uri>
         <TCEforms>
          <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.uri</label>
          <config>
            <type>input</type>
            <size>30</size>
          </config>
          <!-- the &lt;-thing is a hack due to inability of the displayCondition to
                evaluate "user" type fields. greater than 't': sparql, less than 't': uri -->
          <displayCond>FIELD:templateId:&gt;:t</displayCond>
         </TCEforms>
        </uri>

        <sparqlEndpoint>
         <TCEforms>
          <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.sparqlEndpoint</label>
          <config>
            <type>input</type>
            <size>30</size>
          </config>
          <displayCond>FIELD:templateId:&lt;:t</displayCond>
         </TCEforms>
        </sparqlEndpoint>

        <sparqlQuery>
         <TCEforms>
          <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.sparqlQuery</label>
          <config>
            <type>text</type>
          </config>
          <displayCond>FIELD:templateId:&lt;:t</displayCond>
         </TCEforms>
        </sparqlQuery>

        <debugEnabled>
         <TCEforms>
          <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.debugEnabled</label>
          <config>
            <type>check</type>
          </config>
         </TCEforms>
        </debugEnabled>

        <usersParameters>
         <TCEforms>
          <label>LLL:EXT:nr_sw_semantic_templates/locallang_db.xml:tt_content.pi_semantictemplates.usersParameters</label>
          <config>
            <type>input</type>
            <size>50</size>
            <!-- eval>tx_templates_users_parameters_validation</eval -->
          </config>
         </TCEforms>
        </usersParameters>
     </el>
    </ROOT>
   </sDEF>
  </sheets>
</T3DataStructure>
